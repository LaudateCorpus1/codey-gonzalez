<h1 id='start-typing'>Start typing to begin...</h1><br>

<div id="container">

  <div id="prompt-container">
    <pre id="code"></pre>

  </div>
  <div id="timer"></div>

  <div id="results">
    <div id="result-time"></div>
    <div id="wpm"></div>
    <div id="accuracy"></div>
    <div id="score"></div>
  </div>
<br>
<button id='play-again' type="button" onClick="window.location.reload();" class="btn-primary hidden">Play again</button>

</div>
